struct Point {
    x: Int
    y: Int
    z: Int
}


impl Point {
    function positive(self): Int {
        return self.x + self.y + self.z > 0
    }
}


impl Add for Point {
    function add(self, other): Point {
        return Point(
            self.x + other.x,
            self.y + other.y,
            self.z + other.z
        )
    }
}


function main() {
    p1 = Point(1, 2, 3)
    p2 = Point(5, 6, 7)
    print p1 + p2
}
